    {
      "name": "ComboBox",
      "url": "https://developer.microsoft.com/en-us/fabric#/controls/web/combobox",
      "concepts": [
        {
          "name": "allowFreeform",
          "description": "Whether the ComboBox is free form, meaning that the user input is not bound to provided options. Defaults to false."
        },
        {
          "name": "autoComplete",
          "description": "Whether the ComboBox auto completes. As the user is inputing text, it will be suggested potential matches from the list of options."
        },
        {
          "name": "onChange",
          "description": "Callback issued when either: 1) the selected option changes 2) a manually edited value is submitted."
        },
        {
          "name": "onItemClick",
          "description": "Callback issued when a ComboBox item is clicked."
        },
        {
          "name": "onMenuDismiss",
          "description": "Function that gets invoked before the menu gets dismissed"
        },
        {
          "name": "onMenuOpen",
          "description": "Function that gets invoked when the ComboBox menu is launched"
        },
        {
          "name": "scrollSelectedToTop",
          "description": "When options are scrollable the selected option is positioned at the top of the callout when it is opened (unless it has reached the end of the scrollbar)."
        },
        {
          "name": "useComboBoxAsMenuWidth",
          "description": "Whether to use the ComboBoxes width as the menu's width"
        },
        {
          "name": "selectedOptions",
          "description": "All selected values"
        }
      ]
